<p id="notice"><%= notice %></p>

<h1>Schedulings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Starts at</th>
      <th>Ends at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedulings.each do |scheduling| %>
      <tr>
        <td><%= scheduling.title %></td>
        <td><%= scheduling.starts_at %></td>
        <td><%= scheduling.ends_at %></td>
        <td><%= link_to 'Show', scheduling %></td>
      </tr>
    <% end %>
  </tbody>
</table>
